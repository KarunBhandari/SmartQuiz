

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - IQMania</title>

    @*bootstrap*@
    
    <link href="~/lib/twitter-bootstrap/css/bootstrap.css" rel="stylesheet" />
    <link href="~/lib/twitter-bootstrap/css/bootstrap-grid.min.css" rel="stylesheet" />
    <link href="~/lib/jqueryui/themes/ui-lightness/jquery-ui.css" rel="stylesheet" />
    <link href="~/lib/jqueryui/themes/ui-lightness/theme.css" rel="stylesheet" />

    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.6.2/dist/css/bootstrap.min.css" integrity="sha384-xOolHFLEh07PJGoPkLv1IbcEPTNtaed2xpHsD9ESMhqIYd0nLMwNLD69Npy4HI+N" crossorigin="anonymous">


    @*css for datatables*@
    <link href="~/css/datatables.css" rel="stylesheet" />
    <link href="~/css/datatables.min.css" rel="stylesheet" />

    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/IQMania.styles.css" asp-append-version="true" />
   

    @*cssfor ckeditor*@
    <link href="~/lib/ckeditor5-build-classic/sample/css/sample.css" rel="stylesheet" />
    
    
    <script src="~/lib/jquery/jquery.js"></script>
    
    
</head>
<body>
    
    <partial name="_Nav.cshtml" />

    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2023 - IQMania - <a href="https://www.swifttech.com.np/" target="_blank">Developed by Swift Technology</a>
        </div>
    </footer>
    <script src="~/lib/jquery/jquery.js"></script>
    <script src="~/lib/jqueryui/jquery-ui.js"></script>
   

    @*popper*@@*script for bootstrap*@
    
    <script src="~/lib/twitter-bootstrap/js/bootstrap.bundle.js"></script>
    <script src="~/lib/twitter-bootstrap/js/bootstrap.js"></script>
   @* <script src="~/lib/twitter-bootstrap/js/jsdelivr.netpopper.min.js"></script>*@
     <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.6.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-Fy6S3B9q64WdZWQUiU+q4/2Lc9npb8tCaSX9FK7E8HnRr0Jz8D6OP9dO5Vg3Q9ct" crossorigin="anonymous"></script>
   
    
    @*global js*@
    <script src="~/js/site.js" asp-append-version="true"></script>

    @*datatable plugins*@
   <script src="~/lib/jquery/dist/datatables.js"></script>
      
   @*ckeditor*@
    <script src="~/lib/ckeditor5-build-classic/translations/en-gb.js"></script>
    <script src="~/lib/ckeditor5-build-classic/ckeditor.js"></script>
   
   
    @await RenderSectionAsync("Scripts", required: false)

</body>
</html>
